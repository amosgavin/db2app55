                SELECT a.WORKFLOW_ID,
                       a.TEMPLATE_TAG,
                       b.TASK_TAG,
                       (CASE
                         WHEN a.WORKFLOW_OBJECT_TYPE = 'weaponCase' THEN
                          '武器申请'
