<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
  <sysbo datasource="base" name="BOAllTaskStat" mainattr="WORKFLOW_ID">
    <mapingenty type="query">SELECT WORKFLOW_ID,
       TEMPLATE_TAG,
       TASK_TAG,
       WORKFLOW_OBJECT_TYPE,
       OBJECT_TYPE_NAME,
       WORKFLOW_OBJECT_ID,
       CREATE_DATE,
       LABEL,
       TASK_ID,
       TLABEL,
       TASK_DATE,
       STATE_NAME,
       STATE,
       STATION_ID,
       TASK_STAFF_ID,
       TASK_STAFF_NAME,
       ERROR_MESSAGE,
       task_template_id,
       STAFF_NAME,
       FINISH_STAFF_ID,
       FINISH_DATE,
       description,
       REGION_ID,
       ORG_NAME,
       create_staff_name,
       create_time,
       apply_name,
       create_staff_id
  FROM (SELECT WORKFLOW_ID,
               TEMPLATE_TAG,
               TASK_TAG,
               WORKFLOW_OBJECT_TYPE,
               OBJECT_TYPE_NAME,
               WORKFLOW_OBJECT_ID,
               CREATE_DATE,
               LABEL,
               TASK_ID,
               (CASE
                 WHEN TLABEL LIKE '%~%' THEN
                  substr(TLABEL, 1, locate('~', TLABEL, 1) - 1)
                 ELSE
                  TLABEL
               END) TLABEL,
               TASK_DATE,
               STATE_NAME,
               STATE,
               STATION_ID,
               TASK_STAFF_ID,
               TASK_STAFF_NAME,
               ERROR_MESSAGE,
               task_template_id,
               STAFF_NAME,
               FINISH_STAFF_ID,
               FINISH_DATE,
               description,
               REGION_ID,
               ORG_NAME,
               create_staff_id
          FROM (SELECT a.WORKFLOW_ID,
                       a.TEMPLATE_TAG,
                       b.TASK_TAG,
                       (CASE
                         WHEN a.WORKFLOW_OBJECT_TYPE = 'weaponCase' THEN
                          '武器申请'
                         WHEN a.WORKFLOW_OBJECT_TYPE = 'tagCase' THEN
                          '促销标签申请'
                         WHEN a.WORKFLOW_OBJECT_TYPE = 'saleCase' THEN
                          '营销活动申请'
                         ELSE
                          '地市营销活动申请'
                       END) WORKFLOW_OBJECT_TYPE,
                       a.WORKFLOW_OBJECT_TYPE OBJECT_TYPE_NAME,
                       a.WORKFLOW_OBJECT_ID,
                       a.CREATE_DATE,
                       a.LABEL,
                       b.TASK_ID,
                       b.LABEL AS TLABEL,
                       b.CREATE_DATE AS TASK_DATE,
                       (CASE
                         WHEN b.STATE = '5' THEN
                          '等待办理'
                         WHEN b.STATE = '3' THEN
                          '已经完成'
                         WHEN b.STATE = '2' THEN
                          '可调度'
                         WHEN b.STATE = '1' THEN
                          '不能调度'
                         WHEN b.STATE = '4' THEN
                          '终止'
                         WHEN b.STATE = '6' THEN
                          '任务回退'
                         WHEN b.STATE = '10' THEN
                          '任务转派'
                         WHEN b.STATE = '11' THEN
                          '异常结束'
                         ELSE
                          '其他状态'
                       END) STATE_NAME,
                       b.STATE,
                       b.STATION_ID,
                       b.TASK_STAFF_ID,
                       (CASE
                         WHEN task_type = 'user' THEN
                          (SELECT value(sta.STAFF_NAME, '')
                             FROM SECHB.SEC_STAFF sta, SECHB.SEC_OPERATOR ope
                            WHERE sta.STAFF_ID = ope.STAFF_ID
                              AND char(ope.OPERATOR_ID) =
                                  value(b.TASK_STAFF_ID, '0'))
                         WHEN task_type = 'start' THEN
                          (SELECT value(sta.STAFF_NAME, '')
                             FROM SECHB.SEC_STAFF sta, SECHB.SEC_OPERATOR ope
                            WHERE sta.STAFF_ID = ope.STAFF_ID
                              AND char(ope.OPERATOR_ID) =
                                  value(a.create_staff_id, '0'))
                         ELSE
                          ' '
                       END) TASK_STAFF_NAME,
                       b.description ERROR_MESSAGE,
                       b.task_template_id,
                       b.FINISH_STAFF_ID,
                       (SELECT value(sta.STAFF_NAME, '')
                          FROM SECHB.SEC_STAFF sta, SECHB.SEC_OPERATOR ope
                         WHERE sta.STAFF_ID = ope.STAFF_ID
                           AND char(ope.OPERATOR_ID) =
                               value(b.FINISH_STAFF_ID, '0')) STAFF_NAME,
                       b.FINISH_DATE,
                       b.description,
                       a.REGION_ID,
                       (CASE
                         WHEN task_type = 'user' THEN
                          (SELECT value(org.organize_name, '')
                             FROM SECHB.SEC_STAFF    sta,
                                  SECHB.SEC_OPERATOR ope,
                                  SECHB.SEC_ORGANIZE org
                            WHERE sta.STAFF_ID = ope.STAFF_ID
                              AND char(ope.OPERATOR_ID) =
                                  value(b.TASK_STAFF_ID, '0')
                              AND sta.organize_id = org.organize_id)
                         WHEN task_type = 'start' THEN
                          (SELECT value(org.organize_name, '')
                             FROM SECHB.SEC_STAFF    sta,
                                  SECHB.SEC_OPERATOR ope,
                                  SECHB.SEC_ORGANIZE org
                            WHERE sta.STAFF_ID = ope.STAFF_ID
                              AND char(ope.OPERATOR_ID) =
                                  value(a.create_staff_id, '0')
                              AND sta.organize_id = org.organize_id)
                         ELSE
                          ' '
                       END) ORG_NAME,
                       a.create_staff_id
                  FROM hbsale.h_vm_wf a, hbsale.h_VM_TASK b
                 WHERE a.WORKFLOW_ID = b.WORKFLOW_ID
                   AND task_type IN ('user', 'start')
                UNION ALL
                SELECT a.WORKFLOW_ID,
                       a.TEMPLATE_TAG,
                       b.TASK_TAG,
                       (CASE
                         WHEN a.WORKFLOW_OBJECT_TYPE = 'weaponCase' THEN
                          '武器申请'
                         WHEN a.WORKFLOW_OBJECT_TYPE = 'tagCase' THEN
                          '促销标签申请'
                         WHEN a.WORKFLOW_OBJECT_TYPE = 'saleCase' THEN
                          '营销活动申请'
                         ELSE
                          '其他'
                       END) WORKFLOW_OBJECT_TYPE,
                       a.WORKFLOW_OBJECT_TYPE OBJECT_TYPE_NAME,
                       a.WORKFLOW_OBJECT_ID,
                       a.CREATE_DATE,
                       a.LABEL,
                       b.TASK_ID,
                       b.LABEL AS TLABEL,
                       b.CREATE_DATE AS TASK_DATE,
                       (CASE
                         WHEN b.STATE = '5' THEN
                          '等待办理'
                         WHEN b.STATE = '3' THEN
                          '已经完成'
                         WHEN b.STATE = '2' THEN
                          '可调度'
                         WHEN b.STATE = '1' THEN
                          '不能调度'
                         WHEN b.STATE = '4' THEN
                          '终止'
                         WHEN b.STATE = '6' THEN
                          '任务回退'
                         WHEN b.STATE = '10' THEN
                          '任务转派'
                         WHEN b.STATE = '11' THEN
                          '异常结束'
                         ELSE
                          '其他状态'
                       END) STATE_NAME,
                       b.STATE,
                       b.STATION_ID,
                       b.TASK_STAFF_ID,
                       (SELECT value(sta.STAFF_NAME, '')
                          FROM SECHB.SEC_STAFF sta, SECHB.SEC_OPERATOR ope
                         WHERE sta.STAFF_ID = ope.STAFF_ID
                           AND char(ope.OPERATOR_ID) =
                               value(b.TASK_STAFF_ID, '0')) TASK_STAFF_NAME,
                       b.ERROR_MESSAGE,
                       b.task_template_id,
                       b.FINISH_STAFF_ID,
                       (SELECT value(sta.STAFF_NAME, '')
                          FROM SECHB.SEC_STAFF sta, SECHB.SEC_OPERATOR ope
                         WHERE sta.STAFF_ID = ope.STAFF_ID
                           AND char(ope.OPERATOR_ID) =
                               value(b.FINISH_STAFF_ID, '0')) STAFF_NAME,
                       b.FINISH_DATE,
                       b.ERROR_MESSAGE description,
                       a.REGION_ID,
                       (SELECT value(org.organize_name, '')
                          FROM SECHB.SEC_STAFF    sta,
                               SECHB.SEC_OPERATOR ope,
                               SECHB.SEC_ORGANIZE org
                         WHERE sta.STAFF_ID = ope.STAFF_ID
                           AND char(ope.OPERATOR_ID) =
                               value(b.TASK_STAFF_ID, '0')
                           AND sta.organize_id = org.organize_id) ORG_NAME,
                       a.create_staff_id
                  FROM hbsale.h_vm_wf a, hbsale.h_VM_TASK_TS b
                 WHERE a.WORKFLOW_ID = b.WORKFLOW_ID
                   AND task_type IN ('user', 'sign')) c
         ORDER BY CREATE_DATE DESC) data01,
       (SELECT id recordId,
               apply_name,
               principal,
               create_time,
               sta.staff_name create_staff_name
          FROM hbsale.SALE_WEAPON_MAIN_T sw,
               SECHB.SEC_STAFF    sta,
               SECHB.SEC_OPERATOR ope
         WHERE sw.PRINCIPAL = to_char(ope.OPERATOR_ID)
           AND to_char(ope.STAFF_ID) = to_char(sta.STAFF_ID)
        UNION ALL
        SELECT id recordId,
               sale_main_name apply_name,
               principal,
               create_time,
               sta.staff_name create_staff_name
          FROM hbsale.sale_main_t sm, SECHB.SEC_STAFF sta, SECHB.SEC_OPERATOR ope
         WHERE sm.PRINCIPAL = to_char(ope.OPERATOR_ID)
           AND to_char(sta.STAFF_ID) = to_char(ope.STAFF_ID)) data02
 WHERE data01.WORKFLOW_OBJECT_ID = data02.recordId
 
 union
 
